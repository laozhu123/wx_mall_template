/* pages/index/index.wxss */
page {
    height: 100%;
}
.warp {
    height: 100%;
    position: relative;
}

.warp .back {
    width: 100%;
    height: 100%;
}

.warp .rocket{
    width: 160rpx;
    height: 300rpx;
    position: absolute;
    top: 60%;
    left: 43%;
}

.warp .head{
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    position: absolute;
    top: 30rpx;
    left: 30rpx;
}

.warp .index {
    height: 20 px;
    line-height: 20px;
    text-align: center;
    color: #fff;
    position: absolute;
    font-size: 20rpx;
    top: 60rpx;
    left: 110rpx;
    padding-left: 20rpx;
    padding-right: 20rpx;
    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
    background-color: #f5774d
}

.warp .speed_up{
    height: 50px;
    line-height: 50px;
    text-align: center;
    color: #f5774d;
    position: absolute;
    font-size: 25rpx;
    top: 65%;
    left: 70%;
    padding-left: 20rpx;
    padding-right: 20rpx;
    border-radius: 5px 5px 0 5px;
    background-color: #fff
}

.warp .power{
    width: 60px;
    height: 60px;
    position: absolute;
    top: 75%;
    left: 80%;
}
